{
  "expo": {
    "name": "react-native-lv",
    "slug": "react-native-lv",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "reactnativelv",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.josephmw19.reactnativelv",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.josephmw19.reactnativelv"
    },
    "web": {
      "favicon": "src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "src/assets/images/splash-icon.png"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "4f94ab1c-7518-4364-8c41-ed3475c124fe"
      }
    }
  }
}
