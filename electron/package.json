{
  "name": "careconnect-electron-week7",
  "private": true,
  "version": "0.1.0",
  "main": "dist-electron/main.js",
  "scripts": {
    "electron:build": "node scripts/build-electron.mjs",
    "electron:watch": "node scripts/build-electron.mjs --watch",
    "dev": "concurrently -k \"npm run electron:watch\" \"vite\" \"wait-on http://localhost:5173 && wait-on dist-electron/main.js && electron .\"",
    "build": "vite build && npm run electron:build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.13.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.14",
    "@types/react-dom": "^18.3.1",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.0",
    "electron": "^33.2.1",
    "esbuild": "^0.27.3",
    "eslint": "^9.18.0",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "wait-on": "^7.2.0"
  }
}
